import "../script/XmlParserUtils.eol";

rule ELEMENT2ELEMENT
	transform element: Element {
	
	template: "TemplateElement.egl"
	
	target: "../generated/current/src-gen/main/java/flexml/elements/" + element.getElementClassName()+ ".java"
	}

rule ELEMENT2PARSER
	transform element: Element {
	
	template: "TemplateElementParser.egl"
	
	target: "../generated/current/src-gen/main/java/flexml/parser/" + element.getParserClassName() + ".java"
	}
